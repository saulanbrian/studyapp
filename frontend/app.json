{
  "expo": {
    "name": "studyapp",
    "slug": "brian-studyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "studyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.briansaulan.studyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark":{
            "image":"./assets/images/icon-dark.png",
            "backgroundColor":"#1C1C1E"
          }
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b583b81b-c208-49f2-85ab-c9220d78cd1a"
      }
    },
    "owner": "briansaulan",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b583b81b-c208-49f2-85ab-c9220d78cd1a"
    }
  }
}
