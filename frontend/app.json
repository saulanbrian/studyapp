{
  "expo": {
    "name": "Cut D' Crop",
    "slug": "cut-d-crop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cutdcrop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.saulanbrian.cutdcrop"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "react-native-edge-to-edge",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icon-dark.png",
            "backgroundColor": "#1C1C1E"
          }
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f43539bf-dbf1-482f-81ad-aa8bae672aa7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "briansaulan05"
  }
}
